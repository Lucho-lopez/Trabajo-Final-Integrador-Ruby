<div id="<%= dom_id link %>">
  <p>
    <strong>Url:</strong>
    <%= link.url %>
  </p>

  <p>
    <strong>Link type:</strong>
    <%= link.link_type %>
  </p>

  <p>
    <strong>Expires at:</strong>
    <%= link.expires_at %>
  </p>

  <p>
    <strong>Link password:</strong>
    <%= link.link_password %>
  </p>

  <p>
    <strong>Unique token:</strong>
    <%= link.unique_token %>
  </p>

  <p>
    <strong>Access count:</strong>
    <%= link.access_count %>
  </p>

</div>
